{
	"version": 2,
	"builds": [
	  {
		"src": "index.js",
		"use": "@vercel/node"
	  }
	],
	"routes": [
	  {
		"src": "/api/menu",
		"dest": "/menu"
	  },
	  {
		"src": "/api/categories",
		"dest": "/categories"
	  },
	  {
		"src": "/api/menu/:category",
		"dest": "/index.js"
	  },
	  {
		"src": "/api/menu/:category/:product",
		"dest": "/index.js"
	  },
	  {
		"src": "/api/(.*)",
		"dest": "/index.js"
	  }
	]
  }
  